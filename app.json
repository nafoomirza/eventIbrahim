{
  "expo": {
    "name": "EventCast",
    "slug": "eventcast-snack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f0c29"
    },
    "android": {
      "package": "com.nafoomirza.eventcast",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-foreground.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "ios": {
      "bundleIdentifier": "com.nafoomirza.eventcast",
      "supportsTablet": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "userInterfaceStyle": "automatic",
    "extra": {
      "eas": {
        "projectId": "a2c13ab1-9471-4c9c-b30a-58710b00b69a"
      }
    },
    "owner": "nafae",
    "plugins": [
      "expo-font"
    ],
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}